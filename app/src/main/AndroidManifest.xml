    package="com.example.luxianglin.smarthomeofandroid">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Layout.MainActivity" />
        <activity android:name=".Layout.login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Layout.register" />
        <activity android:name=".DataVisualization.EnvironmentStatics" />
        <activity android:name=".DataVisualization.ControlStatics" />
        <activity android:name=".DataVisualization.EnvRecommend"></activity>
    </application>

</manifest>